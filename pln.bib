@article{Lee_1996,
	doi = {10.1086/178062},
	url = {https://doi.org/10.1086%2F178062},
	year = 1996,
	month = {nov},
	publisher = {{IOP} Publishing},
	volume = {472},
	number = {1},
	pages = {275--286},
	author = {Youngung Lee and Ronald L. Snell and Robert L. Dickman},
	title = {The Cold, Massive Molecular Cloud G216-2.5. {III}. Infrared Properties},
	journal = {The Astrophysical Journal},
	abstract = {We present results concerning the star-forming activities and dust properties of the unusual giant molecular cloud G216–2.5. As suggested by earlier CO observations, we confirm that this cloud is unusually cold for a GMC and shows no evidence for massive star formation. The average temperature of the dust based on the extended emission at 60 and 100 μm is 22–26 K. We attribute this low value to the lack of substantial internal heating sources. Dust heating in the cloud is probably dominated by the interstellar radiation field.
 Near-infrared imaging of the  IRAS point sources seen toward G216–2.5 reveal at least four sites of low-luminosity, and presumably low-mass, star formation within the cloud. All four sites lie preferentially toward the edge of the cloud. Two small groupings of stars are identified as the near-infrared counterparts to two of the  IRAS point sources, and the group members may be similar to T Tauri stars. Somewhat more luminous and massive stars appear to be associated with two other  IRAS point sources. Even the most luminous star formation site in G216–2.5 has a far-infrared luminosity of only 300  L☉.}
}



